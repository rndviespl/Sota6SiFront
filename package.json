    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.10",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@taiga-ui/addon-commerce": "^4.35.0",
    "@taiga-ui/addon-table": "^4.36.0",
    "@taiga-ui/cdk": "^4.34.0",
    "@taiga-ui/core": "^4.36.0",
    "@taiga-ui/event-plugins": "^4.0.2",
    "@taiga-ui/icons": "^4.34.0",
    "@taiga-ui/icons-fontawesome": "^4.37.0",
    "@taiga-ui/kit": "^4.34.0",
    "@taiga-ui/layout": "^4.35.0",
    "@taiga-ui/legacy": "^4.35.0",
    "ngx-cookie-service": "^19.1.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.8",
